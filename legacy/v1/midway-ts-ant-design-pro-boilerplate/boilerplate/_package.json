{
  "name": "midway-antd-pro-projet",
  "version": "1.0.0",
  "description": "midway antd pro projet",
  "author": "",
  "scripts": {
    "client-install": "cd client && npm install --no-package-lock",
    "server-install": "cd server && npm install --no-package-lock",
    "postinstall": "npm run client-install && npm run server-install",
    "start": "cd server && npm run start",
    "client-start": "cd client && npm run start",
    "build": "npm run postinstall && npm run client-build && npm run server-build",
    "server-build": "cd server && npm run own-build",
    "client-build": "cd client && npm run build",
    "client-lint": "cd client && npm run lint",
    "server-lint": "cd server && npm run lint",
    "lint": "npm run client-lint && npm run server-lint"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "react",
    "midway",
    "TypeScript"
  ]
}
